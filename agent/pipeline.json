{
  "chainer": {
    "in": ["text", "info"],
    "pipe": [
      {
        "class_name": "components:ApiRequester",
        "url": "ENTITY_DETECTION_URL",
        "in": ["text"],
        "param_names": ["text"],
        "out": ["entity_substr", "template", "context"]
      },
      {
        "class_name": "components:ApiRequester",
        "url": "ENTITY_LINKING_URL",
        "in": ["entity_substr", "template", "context"],
        "param_names": ["entity_substr", "template", "context"],
        "out": ["entity_info_batch"]
      },
      {
        "class_name": "components:ApiRequester",
        "url": "WIKI_PARSER_URL",
        "in": ["entity_info_batch"],
        "param_names": ["query"],
        "out": ["wikidata"]
      },
      {
        "class_name": "components:OutputFormatter",
        "in": ["wikidata", "info"],
        "out": ["formatted_wikidata"]
      }
    ],
    "out": ["formatted_wikidata"]
  },
  "metadata": {
    "server_class": "server:MyCustomServer"
  }
}

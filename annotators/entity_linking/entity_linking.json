{
  "chainer": {
    "in": ["entity_substr", "template", "context"],
    "pipe": [
      {
        "class_name": "processing:EntityLinkingPreprocessor",
        "in": ["entity_substr", "template", "context"],
        "out": ["entity_substr", "template", "long_context", "entity_types", "short_context"]
      },
      {
        "in": ["entity_substr", "template", "long_context", "entity_types", "short_context"],
        "out": ["entity_ids", "confidences", "tokens_match_conf"],
        "config_path": "kbqa_entity_linking_lite.json"
      },
      {
        "class_name": "processing:EntityLinkingPostprocessor",
        "in": ["entity_ids", "confidences", "tokens_match_conf", "entity_substr", "short_context"],
        "out": ["entity_info_batch"]
      }
    ],
    "out": ["entity_info_batch"]
  }
}

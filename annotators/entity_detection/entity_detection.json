{
  "chainer": {
    "in": ["text"],
    "pipe": [
      {
        "in": ["text"],
        "out": ["entities_alexa", "tags_alexa", "positions_alexa", "entities_offsets_alexa", "entity_probas_alexa"],
        "config_path": "dialog_entity_detection.json"
      },
      {
        "in": ["text"],
        "out": ["entities_lc", "tags_lc", "positions_lc", "entities_offsets_lc", "entity_probas_lc"],
        "config_path": "entity_detection_lcquad.json"
      },
      {
        "class_name": "processing:EntityDeduplicator",
        "in": [
          "text",
          "entities_alexa",
          "tags_alexa",
          "entities_offsets_alexa",
          "entities_lc",
          "tags_lc",
          "entities_offsets_lc"
        ],
        "out": ["entity_substr", "template", "context"]
      }
    ],
    "out": ["entity_substr", "template", "context"]
  }
}

{
  "chainer": {
    "in": ["entity_substr", "sentences", "entity_offsets", "entity_tags_with_probas", "sentences_offsets"],
    "pipe": [
      {
        "class_name": "src.entity_linking:EntityLinker",
        "in": ["entity_substr", "sentences", "entity_offsets", "entity_tags_with_probas", "sentences_offsets"],
        "out": ["entity_ids", "entity_tags", "entity_conf", "entity_pages"],
        "load_path": "{DOWNLOADS_PATH}/entity_linking_rus/el_rus_kg_files",
        "tags_filename": "{MODELS_PATH}/el_tags_roberta_rus/tag.dict",
        "rank_in_runtime": true,
        "num_entities_for_conn_ranking": 30,
        "include_mention": false,
        "num_entities_to_return": 5,
        "lemmatize": true,
        "use_tags": true,
        "use_descriptions": true,
        "use_connections": true,
        "full_paragraph": false,
        "return_confidences": true,
        "ignore_tags": true,
        "delete_hyphens": true,
        "log_to_file": true,
        "lang": "ru",
        "return_additional_info": false
      }
    ],
    "out": ["entity_ids", "entity_tags", "entity_conf", "entity_pages"]
  },
  "metadata": {
    "variables": {
      "ROOT_PATH": "~/.deeppavlov",
      "DOWNLOADS_PATH": "{ROOT_PATH}/downloads",
      "MODELS_PATH": "{ROOT_PATH}/models",
      "TRANSFORMER": "DeepPavlov/rubert-base-cased",
      "CONFIGS_PATH": "{DEEPPAVLOV_PATH}/configs"
    },
    "download": [
      {
        "url": "http://files.deeppavlov.ai/deeppavlov_data/entity_linking/el_rus_kg_files.tar.gz",
        "subdir": "{DOWNLOADS_PATH}/entity_linking_rus/el_rus_kg_files/el_rus_kg_files.tar.gz"
      }
    ]
  }
}
